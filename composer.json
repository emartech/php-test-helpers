{
  "name": "emartech/php-test-helpers",
  "description": "Helper classes for testing with phpunit",
  "type": "library",
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "phpunit/phpunit": "^8.4",
    "monolog/monolog": "^2",
    "guzzlehttp/guzzle": "^6.3",
    "emartech/php-jwt-helper": "*",
    "emartech/escher": "^1.2"
  },
  "require-dev": {
    "fqqdk/packagist-publisher": "*"
  },
  "autoload": {
    "psr-4": {
      "Emartech\\TestHelper\\": "src/",
      "Test\\Emartech\\TestHelper\\": "test/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --dont-report-useless-tests test"
  },
  "authors": [
